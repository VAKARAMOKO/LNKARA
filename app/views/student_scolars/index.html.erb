<p id="notice"><%= notice %></p>

<h1>Student Scolars</h1>

<table>
  <thead>
    <tr>
      <th>Total</th>
      <th>Status</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_scolars.each do |student_scolar| %>
      <tr>
        <td><%= student_scolar.total %></td>
        <td><%= student_scolar.status %></td>
        <td><%= student_scolar.student_id %></td>
        <td><%= link_to 'Show', student_scolar %></td>
        <td><%= link_to 'Edit', edit_student_scolar_path(student_scolar) %></td>
        <td><%= link_to 'Destroy', student_scolar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Scolar', new_student_scolar_path %>
